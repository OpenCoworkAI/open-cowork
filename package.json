{
  "name": "open-cowork",
  "version": "2.0.0",
  "description": "Desktop Agent application with sandboxed environment for AI Agent interaction",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "npm run download:node && npm run build:wsl-agent && npm run build:lima-agent && vite",
    "build": "npm run download:node && npm run build:wsl-agent && npm run build:lima-agent && tsc && vite build && electron-builder",
    "build:wsl-agent": "tsc -p src/main/sandbox/wsl-agent/tsconfig.json",
    "build:lima-agent": "tsc -p src/main/sandbox/lima-agent/tsconfig.json",
    "download:node": "node scripts/download-node.js",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "rebuild": "node -e \"const {execSync}=require('node:child_process'); const v=require('electron/package.json').version; execSync('npm rebuild better-sqlite3 --runtime=electron --target='+v+' --disturl=https://electronjs.org/headers',{stdio:'inherit'})\""
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.77",
    "@anthropic-ai/claude-code": "^2.1.7",
    "@anthropic-ai/sdk": "^0.39.0",
    "@larksuiteoapi/node-sdk": "^1.58.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/ws": "^8.18.1",
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^4.0.1",
    "dotenv": "^17.2.3",
    "electron-store": "^11.0.2",
    "glob": "^10.3.10",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "katex": "^0.16.27",
    "lucide-react": "^0.468.0",
    "ngrok": "^5.0.0-beta.2",
    "openai": "^6.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.3",
    "react-markdown": "^9.0.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "uuid": "^11.0.3",
    "webdriverio": "^9.23.2",
    "ws": "^8.19.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^31.7.7",
    "electron-builder": "^26.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.28.8",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "tar": "6.2.1"
  },
  "author": "Open Cowork Team",
  "license": "MIT",
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "utf-8-validate": "^6.0.6"
  }
}
